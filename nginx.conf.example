upstream fjfnaranjo-bot {
    server fjfnaranjo-bot:8001;
}
server {
    listen      443 ssl;
    server_name           example.com;
    ssl_certificate       /botcert/cert.cert;
    ssl_certificate_key   /botcert/key.key;
    location / {
        include uwsgi_params;
        uwsgi_pass fjfnaranjo-bot;
    }
}
